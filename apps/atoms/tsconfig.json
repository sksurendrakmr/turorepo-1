{
    "extends": "@surikmr1/tsconfig/react-library.json",
    "display": "Atoms",
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES5",
        "baseUrl": "src",
        // "rootDir": "./",
        "module": "CommonJS",
        "lib": ["ES5","DOM"],
        "jsx": "react",
        "sourceMap": true,
        "noEmit": false,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "declaration": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }